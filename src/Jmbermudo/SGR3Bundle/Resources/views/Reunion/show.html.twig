{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Reunion</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Nombrepublico</th>
                <td>{{ entity.nombrePublico }}</td>
            </tr>
            <tr>
                <th>Nombreprivado</th>
                <td>{{ entity.nombrePrivado }}</td>
            </tr>
            <tr>
                <th>Descripcion</th>
                <td>{{ entity.descripcion }}</td>
            </tr>
            <tr>
                <th>Fechacreacion</th>
                <td>{{ entity.fechaCreacion|date('Y-m-d H:i:s') }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('reunion_index') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('reunion_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>{{ form(delete_form) }}</li>
</ul>
{% endblock %}
